#include<stdio.h>
void main()
{
	int i,j,k=1,x=7;
	
	for(i=1; i<=7; i++)
	{
		for(j=1; j<=7; j++)
		{
			if(i>=1&&i<=4&&(j==k||j==x)||j==4&&(i>=4&&i<=7))
				printf(" *");
	    	else
	    		printf("  ");
	    		
		}
		k++;
		x--;
		printf("\n");
		
	}
}

