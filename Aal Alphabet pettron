CODE->
#include<stdio.h>
int main()
{
	int i,j;

	for(i=1; i<=8; i++)
	{
		for(j=1; j<=8; j++)
		{
			if(i<=4&&(j>=i&&j<=4+i)||i>=5&&(j>=9-i&&j<=13-i)tr)
				printf(" *");
			else
			 	printf("  ");
		}
		printf("\n");
	}
}

