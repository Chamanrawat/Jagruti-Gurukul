CODE->
#include<stdio.h>
void main()
{
	int i,j,k=1,x=9;
	for(i=1; i<=9; i++)
	{
	//	i<=4 ? k--: 0;
		for(j=1; j<=9; j++)
		{
			if(j==k||j==x)
				printf("  ");
			else
				printf(" *");	
		}
		printf("\n");
		k++;
		x--;
	   // i<4?x++:0;
	}
}

OUTPUT->

   * * * * * * *
 *   * * * * *   *
 * *   * * *   * *
 * * *   *   * * *
 * * * *   * * * *
 * * *   *   * * *
 * *   * * *   * *
 *   * * * * *   *
   * * * * * * *


=======================================================
CODE->
#include<stdio.h>
void main()
{
	int i,j,k=5,x=7,m=6,n=10;
	for(i=1; i<=7; i++)
	{
		i<=4 ? k--: k++;
		i<=4 ? m++: m--;
		for(j=1; j<=13; j++)
		{
			if(j>=k&&j<=x||j>=m&&j<=n)
				printf(" *");
			else
				printf("  ");	
		}
		printf("\n");
	
	    i<=3?x--:x++;
	    i<=3?n++:n--;
	}
	
}

OUTPUT->

       * * * * * * *
     * * * *   * * * *
   * * * *       * * * *
 * * * *           * * * *
   * * * *       * * * *
     * * * *   * * * *
       * * * * * * *
	   
