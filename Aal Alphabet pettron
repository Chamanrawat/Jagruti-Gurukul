CODE->      
#include <stdio.h>

int main()
{
   int i,j,k=1;
   for(i=1; i<=6; i++)
   {
       for(j=1; j<=6; j++)
       {
           if(j==1||j==6||i>=1&&j==k)
                printf(" *");
           else
                printf("  ");
              
       }
       
       printf("\n");
       k++;
    
   }
   
}

			
