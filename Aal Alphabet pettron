 CODE->
 #include<stdio.h>
int main()
{
	int i,j,k=1,x,n;
	printf("Enter a number : ");
	scanf("%d", &n);
	x=n;
	for(i=1; i<=n; i++)
	{
		for(j=1; j<=n; j++)
		{
			if(i>=1&&(j==k||j==x)||i==1||i==(n+1)/2||i==n)
				printf(" *");
			else
			 	printf("  ");
		}
		k++;
		x--;
		printf("\n");
	}
}

OUTPUT->

