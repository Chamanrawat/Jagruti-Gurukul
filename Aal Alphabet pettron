---------------------------------------------------------------------------------
#include<stdio.h>
int main()
{
	int i,j,k=5;

	for(i=1; i<=7; i++)
	{
		i<5 ? k--: k++;
		for(j=1; j<=4; j++)
		{
			if(j>=k)
				printf(" *");
			else
			 	printf("  ");
		}
		printf("\n");
	}
}
OUTPUT->
