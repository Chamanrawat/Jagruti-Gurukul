 CODE->
 
 #include<stdio.h>
void main()
{
	int i,j,k=1,x=11;
	
	for(i=1; i<=6; i++)
	{
		for(j=1; j<=11; j++)
		{
			if(i>=1&&(j==k||j==x))
				printf("*"); 
	    	else
	    		printf(" ");
	    		
		}
		k++;
		x--;
		printf("\n");
		
	}
}

OUTPUT->

