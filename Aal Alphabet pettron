#include<stdio.h>
void main()
{
	int i,j,k=9;
	
	for(i=1; i<=9; i++)
	{
		for(j=1; j<=9; j++)
		{
			if(i==1||(i>=1&&j==k)||i==9)
				printf(" *");
	    	else
	    		printf("  ");
	    		
		}
		k--;
		printf("\n");
		
	}
}

