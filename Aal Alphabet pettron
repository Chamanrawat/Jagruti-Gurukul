int main()
{
    int i,j;
    
	   	for(i=1; i<=7; i++){
        for(j=1;j<=6;j++)
        {
           if(((j==1||j==6)&&(i>=2&&i<=6))||i==7&&(j>=2&&j<=5))
           {
               printf(" *");
           }
           else
           {
               printf("  ");
           }
        
        }
       
        printf("\n");
        
    }
}

