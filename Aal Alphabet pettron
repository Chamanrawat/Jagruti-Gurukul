                           
						   ALL ALFABETS PERTRON
						   ---------------------
						  
							  
CODE->	#include<stdio.h>
		main()
		{
			int i,j;
			
			for(i=1; i<=6; i++)
			{
				for(j=1; j<=5; j++)
				{
					if(((i==1 || i==4) && (j>=2 && j<=4)) || (i>=2 && (j==1 || j==5)))
						printf("*");
					else
						printf(" ");
				}
				printf("\n");
			}
		}

OUTPUT->

		 ***
		*   *
		*   *
		*****
		*   *
		*   *


----------------------------------------------------------------------

CODE ->

		#include<stdio.h>
		main()
		{
			int i,j;
			
			for(i=1; i<=5; i++)
			{
				for(j=1; j<=4; j++)
				{
					  if(i==1 || i==5 ||j==1)
						printf(" C");
					else
						printf(" ");
				}
				printf("\n");
			}
		}
		
output

 C C C C
